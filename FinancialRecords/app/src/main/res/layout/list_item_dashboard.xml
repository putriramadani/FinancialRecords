<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
android:id="@+id/cv_kategori_laporan"
android:layout_width="match_parent"
android:layout_height="wrap_content"
android:layout_marginTop="5dp"
android:layout_marginLeft="15dp"
android:layout_marginRight="15dp"
android:layout_marginBottom="10dp"
app:cardCornerRadius="10dp"
app:cardElevation="4dp"
android:backgroundTint="@color/white"
android:orientation="vertical">

<ImageView
    android:id="@+id/laporan_img"
    android:layout_width="60dp"
    android:layout_height="61dp"
    android:layout_marginStart="15dp"
    android:layout_marginTop="15dp"
    android:layout_marginEnd="10dp"
    android:layout_marginBottom="10dp"
    android:src="@drawable/laporan_img"></ImageView>

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="382dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_weight="1"
        android:background="@color/white"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_weight="1"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingTop="10dp">

            <TextView
                android:id="@+id/tanggal"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="2dp"
                android:text="Tanggal"
                android:textAlignment="gravity"
                android:textColor="@color/orange" />


        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_weight="1"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingStart="10dp">

            <TextView
                android:id="@+id/nama_kategori"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="2dp"
                android:text="Nama Kategori"
                android:textAlignment="gravity"
                android:textColor="@color/orange"
                android:textStyle="normal" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="1dp"
            android:layout_weight="1"
            android:background="@color/white"
            android:orientation="horizontal"
            android:paddingStart="10dp"
            android:paddingBottom="10dp">

            <TextView
                android:id="@+id/keterangan_kategori"
                android:layout_width="130dp"
                android:layout_height="wrap_content"
                android:gravity="left"
                android:padding="2dp"
                android:text="Keterangan"
                android:textAlignment="gravity"
                android:textColor="@color/orange"
                android:textStyle="normal" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_weight="1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"

        android:background="@color/white">

        <TextView
            android:id="@+id/total_kategori"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:gravity="right"
            android:padding="2dp"
            android:text="Total"
            android:textAlignment="gravity"
            android:textColor="@color/orange"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/money_img"
            android:layout_width="41dp"
            android:layout_height="35dp"
            android:layout_marginStart="30dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="8dp"
            android:src="@drawable/moneyrp"></ImageView>

    </LinearLayout>

</LinearLayout>

</androidx.cardview.widget.CardView>